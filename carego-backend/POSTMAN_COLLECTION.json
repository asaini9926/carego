      "name": "1. SETUP - Authentication (Login First)",
      "description": "Start here - Login to get tokens for all subsequent requests",
      "item": [
        {
          "name": "1.1 Admin Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const body = pm.response.json();",
                  "    pm.environment.set('admin_token', body.data.token);",
                  "    pm.environment.set('admin_refresh_token', body.data.refreshToken);",
                  "    pm.environment.set('current_user_id', body.data.user.id);",
                  "    pm.test('Login successful and token saved', () => {",
                  "        pm.expect(body.success).to.be.true;",
                  "        pm.expect(body.data.token).to.exist;",
                  "    });",
                  "}",
                  "pm.test('Status code is 200', () => {",
                  "    pm.response.to.have.status(200);",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@carego.in\",\n  \"password\": \"Test@1234\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "auth",
                "login"
              ]
            }
          }
        },
        {
          "name": "1.2 Client Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const body = pm.response.json();",
                  "    pm.environment.set('client_token', body.data.token);",
                  "    pm.environment.set('client_user_id', body.data.user.id);",
                  "    pm.test('Client login successful', () => {",
                  "        pm.expect(body.data.user.user_type).to.equal('CLIENT');",
                  "    });",
                  "}"
